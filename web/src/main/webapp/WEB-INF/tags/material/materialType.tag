<%@tag pageEncoding="UTF-8" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ attribute name="materialList" required="true" type="java.util.List" %><%@ taglib prefix="materialtype" tagdir="/WEB-INF/tags/material" %><spring:url value="/resources/img/delete_btn.png" var="delete_btn"/><c:forEach var="materialType" items="${materialList}">    <tr>        <td>${materialType.name} </td>        <td>${materialType.pricePerUnit} грн/шт</td>        <td class="hidden">            <form class="controller" action="${pageContext.request.contextPath}/projects/removeMaterialType"                  method="post">                <input type="hidden" name="id" value="${materialType.id}">                <input type="hidden" name="projectId" value="${projectId}">                <input class="delete_btn clickableIcon" type="image" src="${delete_btn}" alt="Delete">            </form>            <materialtype:updateMaterialType materialType="${materialType}"/>        </td>    </tr></c:forEach>