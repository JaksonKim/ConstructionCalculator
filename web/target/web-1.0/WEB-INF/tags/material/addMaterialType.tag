<%@tag pageEncoding="UTF-8" %><%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ attribute name="materialTypeGroup" required="true" type="ua.ksstroy.logic.material.MaterialTypeGroup" %><spring:url value="/resources/img/done_btn.png" var="done_btn"/><div class="controller">    <div class="controller_name">ТИП МАТЕРИАЛА</div>    <form action="${pageContext.request.contextPath}/projects/addMaterialType" method="post">        <input type="hidden" name="parentId" value="${materialTypeGroup.id}">        <input type="hidden" name="projectId" value="${projectId}">        <table>            <tbody>            <tr>                <td>Имя:</td>                <td><input type="text" name="name" placeholder="Введите имя"></td>            </tr>            <tr>                <td>Описание:</td>                <td><input type="text" name="description" placeholder="Введите описание"></td>            </tr>            <tr>                <td>Цена:</td>                <td><input type="text" name="pricePerUnit" placeholder="Введите цену (грн/шт)"></td>            </tr>            <tr>                <td>Имя единицы:</td>                <td><input type="text" name="unitName" placeholder="Введите имя единицы"></td>            </tr>            </tbody>        </table>        <input class="accept_changes_btn clickableIcon" type="image" src="${done_btn}">    </form></div>