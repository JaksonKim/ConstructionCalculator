<%@tag pageEncoding="UTF-8"%><%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ attribute name="materialType" required="true" type="ua.ksstroy.logic.material.MaterialType" %><spring:url value="/resources/img/done_btn.png" var="done_btn"/><div class="controller">    <div class="controller_name">ОБНОВИТЬ ТИП МАТЕРИАЛА</div>    <form action="${pageContext.request.contextPath}/projects/updateMaterialType" method="post">        <input type="hidden" name="id" value="${materialType.id}">        <input type="hidden" name="projectId" value="${projectId}">        <table>            <tbody>            <tr>                <td>Имя:</td>                <td><input type="text" name="name" value="${materialType.name}"></td>            </tr>            <tr>                <td>Описание:</td>                <td><input type="text" name="description" value="${materialType.description}"></td>            </tr>            <tr>                <td>Цена:</td>                <td><input type="text" name="pricePerUnit" value="${materialType.pricePerUnit}"></td>            </tr>            <tr>                <td>Имя единицы:</td>                <td><input type="text" name="unitName" value="${materialType.unitName}"></td>            </tr>            </tbody>        </table>        <input class="accept_changes_btn clickableIcon" type="image" src="${done_btn}">    </form></div>