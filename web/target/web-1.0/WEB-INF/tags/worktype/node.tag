<%@tag pageEncoding="UTF-8" %><%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="worktype" tagdir="/WEB-INF/tags/worktype" %><%@ attribute name="workTypeGroup" required="true" type="ua.ksstroy.logic.worktype.WorkTypeGroup" %><worktype:workTypeGroup workTypeGroup="${workTypeGroup}"/><worktype:workTypes workTypeList="${workTypeGroup.workTypes}"/><c:forEach var="innerMaterialTypeGroup" items="${workTypeGroup.workTypeGroups}">    <worktype:node workTypeGroup="${innerMaterialTypeGroup}"/></c:forEach>