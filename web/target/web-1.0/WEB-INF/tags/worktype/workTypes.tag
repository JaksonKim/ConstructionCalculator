<%@tag pageEncoding="UTF-8" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ attribute name="workTypeList" required="true" type="java.util.List" %><%@ taglib prefix="worktype" tagdir="/WEB-INF/tags/worktype" %><spring:url value="/resources/img/delete_btn.png" var="delete_btn"/><c:forEach var="workType" items="${workTypeList}">    <tr>        <td>${workType.name} </td>        <td>${workType.unitPrice} грн/м²</td>        <td class="hidden">            <form class="controller" action="${pageContext.request.contextPath}/projects/removeWorkType" method="post">                <input type="hidden" name="id" value="${workType.id}">                <input type="hidden" name="projectId" value="${projectId}">                <input class="delete_btn clickableIcon" type="image" src="${delete_btn}" alt="Delete">            </form>            <worktype:updateWorkType workType="${workType}"/>        </td>    </tr></c:forEach>